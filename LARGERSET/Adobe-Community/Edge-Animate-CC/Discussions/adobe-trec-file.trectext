var answer = sym.$("answer"); answer.html("answer 1"); inputanswer = $('<input />').attr({'type':'text', 'value':'', 'id':'answer'}); inputanswer .css ('font-size', 14); inputanswer .css ('width', 150); inputanswer .css ('background-color','#ffffff'); inputanswer .appendto(answer); 
