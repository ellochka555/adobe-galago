 folioid: folio_id, itemid: 16, title: "wp14", description: "new issues in human resource management." }, { folioid: folio_id, itemid: 17, title: "wp15", description: "innovation management." }, { folioid: folio_id, itemid: 18, title: "wp16", description: "marketing strategies." }, { folioid: folio_id, itemid: 19, title: "wp21", description: "research in tourism management." }, { folioid: folio_id, itemid: 20, title: "wp22", description: "challenges for small and medium enterprises." }, { folioid: folio_id, itemid: 21, title: "wp23", description: "service quality and delivery." }, { folioid: folio_id, itemid: 22, title: "wp24", description: "ict application strategies." }, { folioid: folio_id, itemid: 23, title: "wp25", description: "technology and customer involvement." }, { folioid: folio_id, itemid: 24, title: "wp26", description: "symposium: contemporary issues in reward and remuneration." }, { folioid: folio_id, itemid: 25, title: "ra11", description: "challenges in tourism management." }, { folioid: folio_id, itemid: 26, title: "ra12", description: "research in entrepeneurship." }, { folioid: folio_id, itemid: 27, title: "ra13", description: "ethics in organization." }, { folioid: folio_id, itemid: 28, title: "ra14", description: "tourism, culture and natural resources." }, { folioid: folio_id, itemid: 29, title: "ra15", description: "symposium: talent management of academic staff in south african higher education institutions." }, { folioid: folio_id, itemid: 30, title: "ra21", description: "innovation in education." }, { folioid: folio_id, itemid: 31, title: "ra22", description: "research in international finance." }, { folioid: folio_id, itemid: 32, title: "ra23", description: "topics in tourism and hospitality management." }, { folioid: folio_id, itemid: 33, title: "ra24", description: "hospitality and tourism." }, { folioid: folio_id, itemid: 34, title: "ra25", description: "issues in training and education." }, { folioid: folio_id, itemid: 35, title: "special panel of educational leaders 1", description: "global challenges in talent development." }, { folioid: folio_id, itemid: 36, title: "special panel of educational leaders 2", description: "positioning research-informed globalization." }, { folioid: folio_id, itemid: 37, title: "special panel", description: "optimizing economic development drivers." }, { folioid: folio_id, itemid: 38, title: "special panel continued", description: "optimizing economic development drivers - including perspectives from china." } ];  // values used when creating the database. var name = "note_items"; var version = "1"; var description = "stores items that users can take notes on."; var size = 100000;  var db = opendatabase(name, version, description, size); var initresulthandler; var initerrorhandler;  function init(resulthandler, errorhandler) { initresulthandler = resulthandler; initerrorhandler = errorhandler; createdatabase() }  // only called if the sql statement returns an error which means the tables do not exist. function createdatabase() { // add the table. db.transaction( function(tx) { tx.executesql("drop table if exists items;"); tx.executesql( "create table items (folioid varchar(100), itemid varchar(100), title text, description text, notes text)", null, function(tx, result) { if (initresulthandler) initresulthandler(); } ); } );  // add the items to the database.</s><s> the note fields will be blank. db.transaction( function(tx) { for (var i = 0; i < records.length; i++) { var record = records[i]; tx.executesql("insert into items (folioid, itemid, title, description) values ('" + record.folioid + "','" + record.itemid + "','" + record.title + "','" + record.description + "')"); } } ); }  return { // this must be called before the delegate can be used. // callbacks are used in case the database has not been // created yet. init: function(resulthandler, errorhandler) { init(resulthandler, errorhandler); },  // returns the data for an item. getitem: function(folioid, itemid, resulthandler, errorhandler) { db.transaction( function(tx) { tx.executesql( "select * from items where folioid = ?</s><s> and itemid = ?", [folioid, itemid], function(tx, result) { if (resulthandler) resulthandler(result); }, function(tx, error) { if (errorhandler) errorhandler(error); } ); } ); },  // saves a note for an item to the database. savenote: function(folioid, itemid, notes) { db.transaction( function(tx) { tx.executesql("update items set notes = ?</s><s> where folioid = ?</s><s> and itemid = ?", [notes, folioid, itemid]) } ) },  // gets all of the items and notes from the database. getnotes: function(folioid, resulthandler, errorhandler) { // cast to a string in case folioid is a number since the datatype is a varchar. folioid = string(folioid); db.transaction( function(tx) { tx.executesql( "select * from items where folioid = ?</s><s> and notes <> ''", [folioid], function(tx, result) { if (resulthandler) resulthandler(result); }, function(tx, error) { if (errorhandler) errorhandler(error); } ); } ); },  // removes the notes for an item. removenote: function(folioid, itemid, resulthandler, errorhandler) { // cast to a string in case folioid is a number since the datatype is a varchar. folioid = string(folioid); db.transaction( function(tx) { tx.executesql( "update items set notes = '' where folioid = ?</s><s> and itemid = ?", [folioid, itemid], function(tx, result) { if (resulthandler) resulthandler(result); }, function(tx, error) { if (errorhandler) errorhandler(error); } ); } ) },  // removes notes for all items. removeallnotes: function(folioid, resulthandler, errorhandler) { // cast to a string in case folioid is a number since the datatype is a varchar. folioid = string(folioid); db.transaction( function(tx) { tx.executesql( "update items set notes = '' where folioid = ?", [folioid], function(tx, result) { if (resulthandler) resulthandler(result); }, function(tx, error) { if (errorhandler) errorhandler(error); } ); } ) } } }()); 
